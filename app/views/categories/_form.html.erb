<div class="thumbnail">
  <h2>CATEGORIES</h2>
</div>
<div class="thumbnail">
<%= form_for(@category) do |f| %>
 <% if @category.errors.any? %>
      <ul>
        <% @category.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
  <% end %>
  <p>
    <%= f.label :code %><br>
    <%= f.text_field :code %>
  </p>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
</div>