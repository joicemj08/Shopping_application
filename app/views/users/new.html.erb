<div class="row">
  <h2>USERS</h2>
</div>
<div class="thumbnail">
<%= form_for (@user) do |f| %>
  <p>
    <%= f.label :First_Name %><br>
    <%= f.text_field :first_name %>
  </p>
  <p>
    <%= f.label :Last_Name %><br>
    <%= f.text_field :last_name %>
  </p>
   <p>
    <%= f.label :Email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
</div>
