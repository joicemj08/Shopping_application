
#mainWrap {
    min-height: 100%;
    overflow: auto;
    padding-bottom: 96px;
}

body{
    background: url("<%= asset_path('80.jpg')%>") repeat scroll 0% 0%;
    color: #333333;
    background-color: #ffffff;
}

.items-listing{

    padding: 50px !important;
    border-radius: 5px;
}
#display-products{
  background-color: lightgrey;

    padding: 25px;
    border: 5px solid navy;
    height: 100px;
    /*margin: 25px;*/
}
#loggit {
    height: 275px;
    background: transparent url("<%= asset_path('75.png')%>") repeat scroll 0% 0%;
    width: 480px;
    margin: 5% auto 0px;
    padding: 20px 40px;
}

#loggit .checkbox-style{
    padding-left: 5px !important;
}

#loggit h1 {
    text-align: center;
    font-weight: 700;
    margin: 5px 0px 15px;
}

#loggit h3 {
    text-align: center;
    font-size: 18px;
    color: #BBB;
    margin: 0px 0px 20px;
}

#loggit .input-group-addon {
    border: 0px none;
}

#loggit .form-control {
    border: 0px none;
}

#loggit .formSubmit {
    margin-bottom: 25px;
}

#loggit .formNotice {
    margin: 0px;
    font-size: 13px;
}

.active {
    color: #FFF;
    background-color: #337AB7;
}

.active a {
    color: #FFF;
}

.not-visible {
    display: none !important;
}

.pagination {

  margin: 18px 0;

}
.pagination span{
  background: #fff;
  padding: 5px;
  margin: 0px 3px;
}
.pagination ul {
  display: inline-block;
  *display: inline;
  /* IE7 inline-block hack */

  *zoom: 1;
  margin-left: 0;
  margin-bottom: 0;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
}

.pagination li {
  display: inline;
}
.product-layout{
  font-family: "Open Sans",sans-serif;
font-weight: 400;
color: rgb(102, 102, 102);
font-size: 14px;
line-height: 20px;
background-color: #fff;
}

